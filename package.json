{
  "name": "obsidian-reminder-plugin",
  "version": "1.1.12",
  "description": "Reminder plugin for Obsidian",
  "main": "main.js",
  "scripts": {
    "dev": "rollup --config rollup.config.js -w",
    "build": "cross-env NODE_OPTIONS='--no-deprecation' rollup --config rollup.config.js --environment BUILD:production",
    "test": "jest"
  },
  "keywords": [],
  "author": "uphy",
  "license": "MIT",
  "devDependencies": {
    "@codemirror/commands": "^6.1.2",
    "@codemirror/language": "^6.3.0",
    "@codemirror/search": "^6.2.2",
    "@codemirror/state": "^6.1.2",
    "@codemirror/stream-parser": "https://github.com/lishid/stream-parser",
    "@codemirror/view": "^6.4.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@tsconfig/svelte": "^2.0.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^14.14.37",
    "cross-env": "^7.0.3",
    "electron": "^21.2.0",
    "jest": "^27.0.6",
    "moment": "^2.29.1",
    "obsidian": "^0.16.3",
    "rollup": "^2.32.1",
    "rollup-plugin-svelte": "^7.1.0",
    "svelte-check": "^2.2.4",
    "svelte-jester": "^2.0.1",
    "svelte-preprocess": "^4.7.4",
    "ts-jest": "^27.0.5",
    "tslib": "^2.2.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "rrule": "^2.6.8",
    "svelte": "^3.42.1"
  },
  "overrides": {
    "@electron/get": "2.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testEnvironment": "jsdom",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/*.test.ts"
    ],
    "preset": "ts-jest"
  }
}
